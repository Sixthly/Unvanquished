+
+  Com_sprintf( name, sizeof(name), "./../._FONT_%d", img );
+  //{static int n = 0; Com_sprintf( name, sizeof(name), "./../._FONT_%d", n++ );}
+
+  image = R_CreateImage(name, imageBuf, 32, 32, qfalse, qfalse, GL_CLAMP_TO_EDGE);
+  face->images[ img ] = (void *) image;
